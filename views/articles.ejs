<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Lemur Learns</title>
</head>
<body>
<nav><a href="/articles/edit">edit</a></nav>

<% articles.forEach( function(article) { %>

<summary>
  <h2><%= article.title %></h2>
  <h3><%= article.created %></h3>
  <img src="<%= article.image_url %>">
  <p><%= article.summary %></p>
  <a href="/article/<%= article.id %>">read more</a>
</summary>

<% }); %>


</body>
</html>